{
  "version": 2,
  "builds": [
    {
      "src": "musidle-frontend/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "musidle-api/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://localhost:5000/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app/$1"
    }
  ]
}
